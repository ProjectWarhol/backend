{
  "name": "unos-backend",
  "version": "1.0.0",
  "description": "unos backend",
  "main": "app.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --timeout 5000",
    "test:dev": "NODE_ENV=test npm run test ./test/*.test.js",
    "dev": "NODE_ENV=development nodemon --trace-warnings app.js",
    "start": "node app.js",
    "prod": "NODE_ENV=production NODE_EXTRA_CA_CERTS=/var/www/production/ca-certificate.crt node app.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "setup:db": "sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "setup:prod:db": "NODE_ENV=production sequelize db:migrate && sequelize db:seed:all",
    "setup:testing": "NODE_ENV=test npm run setup:db",
    "lint:sol": "solhint './app/blockchain/contracts/**/*.sol'"
  },
  "lint-staged": {
    "**/*.js": [
      "npm run format",
      "npm run lint"
    ]
  },
  "keywords": [
    "unos"
  ],
  "author": "Unos team",
  "license": "ISC",
  "dependencies": {
    "@truffle/hdwallet-provider": "^2.0.3",
    "babel-eslint": "^10.1.0",
    "bcrypt": "^5.0.1",
    "bip39": "^3.0.4",
    "body-parser": "^1.19.0",
    "chai-as-promised": "^7.1.1",
    "connect-session-sequelize": "^7.1.2",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "ethers": "^5.6.2",
    "express": "^4.17.2",
    "express-fileupload": "^1.3.1",
    "express-rate-limit": "^6.3.0",
    "express-session": "^1.17.2",
    "express-validator": "^6.12.1",
    "faker": "^5.5.3",
    "fs": "^0.0.1-security",
    "helmet": "^4.6.0",
    "ioredis": "^5.0.4",
    "morgan": "^1.10.0",
    "nft.storage": "^6.0.2",
    "nodemailer": "^6.6.3",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "rate-limit-redis": "^3.0.1",
    "sanitize": "^2.1.0",
    "sequelize": "^6.6.5",
    "web3": "^1.7.1",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "chai-json-schema": "^1.5.1",
    "chai-like": "^1.1.1",
    "chai-things": "^0.2.0"
  },
  "devDependencies": {
 
    "deep-equal-in-any-order": "^1.1.15",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "sequelize-cli": "^6.2.0",
    "sinon": "^11.1.2",
    "solhint": "^3.3.7",
    "supertest": "^6.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ProjectWarhol/backend"
  },
  "bugs": {
    "url": "https://github.com/ProjectWarhol/backend/issues"
  },
  "homepage": "https://github.com/ProjectWarhol/backend#readme"
}
